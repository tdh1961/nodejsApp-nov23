pipeline {
    agent {docker {image 'node:14'
            args '-u root:sudo'}}
    
    stages {
        stage ("Dep install"){
            steps {
                //sh 'sudo shown 0755 npm'
                sh 'npm install'
            }
        }
        stage ("npm package"){
            steps{
                sh 'npm pack'
            }
        }
    }

}